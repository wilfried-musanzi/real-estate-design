{
  "name": "agency",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.0",
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.2.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.1.0",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@symfony/webpack-encore": "^2.1.0",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.0.0",
    "node-sass": "^7.0.1",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "sass-loader": "^12.0.0",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@hotwired/turbo": "^7.1.0",
    "@yaireo/tagify": "^3.25.0",
    "alpinejs": "^3.10.2",
    "bootstrap": "^5.1.3",
    "bootstrap-datepicker": "^1.9.0",
    "chart.js": "^2.9.4",
    "clipboard": "^2.0.8",
    "code-prettify-google": "^1.0.1",
    "datatables.net": "^1.10.25",
    "datatables.net-bs4": "^1.10.25",
    "datatables.net-buttons-bs4": "^1.7.1",
    "datatables.net-responsive": "^2.2.9",
    "datatables.net-responsive-bs4": "^2.2.9",
    "dragula": "^3.7.3",
    "dropzone": "^5.9.2",
    "dual-listbox": "^1.4.0-alpha3",
    "fullcalendar": "^5.7.2",
    "htmx.org": "^1.7.0",
    "jkanban": "^1.3.1",
    "jquery": "^3.6.0",
    "jquery-form": "^4.2.2",
    "jquery-timepicker": "^1.3.3",
    "jquery-validation": "^1.19.3",
    "jqvmap": "^1.5.1",
    "jstree": "^3.3.11",
    "jszip": "^3.7.0",
    "knob": "^1.1.0",
    "luxon": "^2.4.0",
